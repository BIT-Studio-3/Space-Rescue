				float4 triplanar17 = TriplanarSampling17( _DetailMap, IN.ase_texcoord8.xyz, IN.ase_normal, _TriplanarFalloff, _DetailTiling, 1.0, 0 );
				float4 detail41 = triplanar17;
				float4 break45 = detail41;
				float detailTexture50 = pow( ( break45.x * break45.y ) , 0.5 );
				float detaledHeight61 = ( height40.x + ( ( detailTexture50 - 0.5 ) * 2.0 * _IceDetail ) );
				float temp_output_2_0_g46 = ( _IceFactorA - _IceDetail );
				float detaledHeightLinStep66 = saturate( saturate( ( ( detaledHeight61 - temp_output_2_0_g46 ) / ( ( _IceDetail + _IceFactorB ) - temp_output_2_0_g46 ) ) ) );
